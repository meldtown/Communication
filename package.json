{
  "name": "hub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:db\" \"npm run start:hub\"",
    "start:hub": "webpack-dev-server --env hub",
    "start:db": "json-server --watch db.json --port 8181",
    "dist": "webpack -p",
    "test": "mocha --reporter spec --require babel-register --require ignore-styles --require jsdom-global/register \"src/**/*.spec.js\"",
    "cover": "nyc npm test",
    "generate": "babel-node db.js",
    "postinstall": "npm run generate"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "faker": "^3.1.0",
    "html-loader": "^0.4.4",
    "ignore-styles": "^5.0.1",
    "jquery-mockjax": "^2.2.1",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "json-server": "^0.8.22",
    "mocha": "^3.1.0",
    "node-sass": "^3.10.0",
    "nyc": "^8.3.0",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.8"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "knockout": "^3.4.0"
  }
}
